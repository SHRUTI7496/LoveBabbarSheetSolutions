import java.util.*;
public class Main
{
	public static void main(String[] args) {
	int arr[] = new int []{-1,-2,-3,-4};
int n = arr.length;
	int sum=Integer.MIN_VALUE;
	for(int i =0;i<arr.length;i++){
	    int csum = 0;
	     for (int j = i; j <= n - 1; j++) {
            csum += arr[j];
            if (csum > sum) {
                sum = csum;
            }
	     
	     
	    }
	}
	System.out.println(sum);
	
	}
	}
