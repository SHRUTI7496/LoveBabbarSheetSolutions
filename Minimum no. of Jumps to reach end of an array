// Need to optimize below code , throwing time limit exceed in GFG.

import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
int [] arr= new int[]{1, 3, 5, 8, 9, 2, 6, 7, 6, 8, 9};
int count=0;
 int n= arr.length;
        int totalhead=0;
        while(totalhead<n-1){
            count = count+1;
            totalhead=totalhead+arr[totalhead];
 
          
        }
    
	System.out.println(count);
	}
}
  
