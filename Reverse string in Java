
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		String s="Shruti";
		String s1="";
		char[] ch=s.toCharArray();
		char []chrev= new char[ch.length];
		int j=0;
		for(int i =ch.length-1;i>=0;i--){
		    chrev[j]= ch[i];
		    j++;
		}
		for(int i =0;i<chrev.length;i++){
	    s1=s1+chrev[i];
	}
	System.out.println(s1);
	}
	
}
