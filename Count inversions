//https://practice.geeksforgeeks.org/problems/inversion-of-array-1587115620/1 
// Need to optimize. Failing due to timeframe


import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
int [] arr= new int[]{2, 4, 1, 3, 5};
int N=arr.length;
long count=0;
        for(int i =0;i<N-1;i++){
            for(int j=i+1;j<N;j++){
            if(arr[i]>arr[j]){
                count++;
            }
        }
        }
      System.out.println(count);
}
}
